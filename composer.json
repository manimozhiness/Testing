{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-dompdf": "^0.8.0",
        "consoletvs/charts": "4.*",
        "dompdf/dompdf": "^0.8.0",
        "guzzlehttp/guzzle": "~6.0",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "maatwebsite/excel": "~2.1.0",
        "nesbot/carbon": "^1.22",
       
        "symfony/psr-http-message-bridge": "^1.0",
        "tymon/jwt-auth": "^0.5.11",
        "zendframework/zend-diactoros": "^1.4",
        "zizaco/entrust": "^1.8"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Helper/PnrRequestHelper.php",
            "app/Helper/CEHelper.php",
            "app/Helper/JCEHelper.php",
            "app/Helper/CoordinatorHelper.php",
            "app/Helper/RequesterHelper.php",
            "app/Helper/CEHelper.php",
            "app/Helper/commodityHelper.php",
            "app/Helper/ReportHelper.php",
            "app/Helper/SearchHelper.php",
            "app/Helper/ChartHelper.php"

        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
